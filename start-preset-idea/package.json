    "octopus-start-modules-tasks": "~0.0.1",
    "octopus-start-tasks": "~0.0.1"
  },
  "peerDependencies": {
    "start": ">= 5.1.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "3.19.0",
    "eslint-plugin-mocha": "4.9.0",
